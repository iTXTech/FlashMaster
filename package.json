    "vue": "^2.6.10",
    "vue-clipboard2": "^0.3.1",
    "vue-i18n": "^8.15.1",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.13",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
